_satellite.pushAsyncScript(function(event, target, $variables){
  var KAMPYLE_INJECT = {
   kampyleInit: function() {
       var t = document.createElement('script');
       t.type = 'text/javascript';
       t.async = true;
       t.src = "https://resources.digital-cloud.medallia.eu/wdceu/6912/onsite/embed.js";
       var s = document.body.appendChild(t);

     var elements = document.getElementsByClassName("oo_feedback_float");
       if (elements){
         for (var i = 0; i < elements.length; i++){
           elements[i].style.display = "none";
         }
       }
   },
   kampyleEventHandler: function(elem, eventType, handler) {
       if (elem.addEventListener) {
           elem.addEventListener(eventType, handler, false);
       } else if (elem.attachEvent) {
           elem.attachEvent('on' + eventType, handler);
       }
   },
};
if (document.readyState === "complete") {
   KAMPYLE_INJECT.kampyleInit();

} else {
   KAMPYLE_INJECT.kampyleEventHandler(window, 'load', KAMPYLE_INJECT.kampyleInit);
}

});
