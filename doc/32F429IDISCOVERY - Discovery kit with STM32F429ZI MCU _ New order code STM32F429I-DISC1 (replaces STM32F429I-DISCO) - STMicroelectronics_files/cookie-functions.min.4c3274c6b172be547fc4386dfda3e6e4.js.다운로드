W3C={};
W3C.CookieFunctions={};
W3C.CookieFunctions.setCookie=function setCookie(b,h,c){var f=typeof isActiveCookie==="function";
if(f&&!isActiveCookie(b)){return
}var g=new Date();
g.setTime(g.getTime()+(c*24*60*60*1000));
var a=";expires="+g.toUTCString();
var e=";path=/";
document.cookie=b+"="+h+a+e
};
W3C.CookieFunctions.getCookie=function getCookie(d){var f=typeof isActiveCookie==="function";
if(f&&!isActiveCookie(d)){return""
}var b=d+"=";
var a=document.cookie.split(";");
for(var e=0;
e<a.length;
e++){var g=a[e];
while(g.charAt(0)==" "){g=g.substring(1)
}if(g.indexOf(b)==0){return g.substring(b.length,g.length)
}}return""
};
W3C.CookieFunctions.trimParenthesis=function trimParenthesis(b){if(b==void (0)||b==""){return""
}var c='"';
var a=new RegExp(c,"g");
return b.replace(a,"")
};
W3C.CookieFunctions.appendCookie=function appendCookie(b,e,c){var a=W3C.CookieFunctions.trimParenthesis(W3C.CookieFunctions.getCookie(b));
if(a!=""&&void (0)!=e&&e!=""){if(a.indexOf(e)>-1){return
}var d=a+","+e;
W3C.CookieFunctions.setCookie(b,d,c)
}else{if(a==""){W3C.CookieFunctions.setCookie(b,e,c)
}}};
W3C.CookieFunctions.appendJsonCookie=function(c,f,e){var a=W3C.CookieFunctions.getCookie(c);
if(a!=""&&void (0)!=f&&f!=""){var b=Object.keys(f)[0];
if(a.indexOf(b)>-1){return
}var d=JSON.parse(a);
d=$.extend(d,f);
W3C.CookieFunctions.setCookie(c,JSON.stringify(d),e)
}else{if(a==""){W3C.CookieFunctions.setCookie(c,JSON.stringify(f),e)
}}};